{
  "name": "parkdude-backend",
  "version": "0.1.0",
  "bin": {
    "parkdude-backend": "build/bin/parkdude-backend.js"
  },
  "scripts": {
    "build": "tsc",
    "postinstall": "ts-node bin/copy-node-modules.ts",
    "watch": "tsc -w",
    "dev": "nodemon --watch \"app/**/*.ts\" --ignore \"app/**/*.test.ts\" --ext ts,env --exec \"ts-node app/index.ts\"",
    "dev:serve": "ts-node app/index.ts",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --runInBand --watch",
    "lint": "eslint --ext .ts app/ lib/ bin/",
    "lint-test": "eslint app/app.ts",
    "lint:fix": "eslint --fix app/**/*.ts handlers/**/*.ts lib/**/*.ts bin/**/*.ts",
    "cdk": "cdk",
    "synth": "cdk synth",
    "local-synth": "cdk synth --no-staging > template.yaml",
    "sam-api": "cdk synth --no-staging > template.yaml && sam local start-api --docker-network=host",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:generate": "npm run typeorm migration:generate -- --dir app/migrations --config env/app.dev.env --name",
    "typeorm:show": "npm run typeorm migration:show -- --config env/app.dev.env",
    "typeorm:migrate": "npm run typeorm migration:run -- --config env/app.dev.env",
    "typeorm:revert": "npm run typeorm migration:revert -- --config env/app.dev.env"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.11.0",
    "@types/jest": "^24.0.18",
    "@types/supertest": "^2.0.8",
    "aws-cdk": "^1.11.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^10.0.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "~3.6.2"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.11.0",
    "@aws-cdk/aws-lambda": "^1.11.0",
    "@aws-cdk/core": "^1.11.0",
    "@types/aws-lambda": "^8.10.33",
    "@types/aws-serverless-express": "^3.3.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express-session": "^1.15.15",
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.7.11",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/rimraf": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "aws-serverless-express": "^3.3.6",
    "class-validator": "^0.10.1",
    "connect-typeorm": "^1.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "eslint": "^6.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "fs-extra": "^8.1.0",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.9",
    "superagent": "^5.1.0",
    "typeorm": "^0.2.19",
    "typeorm-static": "^1.1.1"
  }
}